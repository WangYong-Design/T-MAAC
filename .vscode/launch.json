{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--alg",
                "icstransmaddpg",
                "--alias",
                "embed_24",
                "--mode",
                "distributed",
                "--scenario",
                "case322_3min_final",
                "--qweight",
                "0.1",
                "--voltage-barrier-type",
                "l2",
                "--save-path",
                "trial",
                "--gpuid",
                "2"
            ]
        }
    ]
}

